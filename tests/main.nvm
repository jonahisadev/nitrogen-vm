#entry main

#section TEXT

main:
	ldd eax $x
	ldb ebx $y
	ldw ecx $z
	
	add eax ebx
	add eax ecx
	
	; Function Call Standard
	pusha
	store eax
	call @print_num
	add esp 4
	popa
	
	exit
	
print_num:
	get eax (ebp)+8
	store eax
	ncall %print_num
	add esp 4
	
	const 10
	ncall %print_char
	add esp 4
	
	ret

#section DATA

x: dd 1209
y: db 0x2A
z: dw 360